<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="UI.Maui.Main.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    
    xmlns:local="clr-namespace:UI.Maui.Main"
    xmlns:views="clr-namespace:UI.Maui.Main.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="Dial Hard">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate views:ContactsPage}"
        Route="ContactsPage" />

    <ShellContent
        Title="Add Contact"
        ContentTemplate="{DataTemplate views:AddContactPage}"
        Route="AddContactPage" />


</Shell>
